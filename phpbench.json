{
    "storage": "xml",
    "bootstrap": "vendor/autoload.php",
    "path": "tests/Benchmarks",
    "extensions": [
        "PhpBench\\Extensions\\XDebug\\XDebugExtension"
    ],
    "php_disable_ini": true,
    "php_config": {
        "extension": [
            "tokenizer",
            "json"
        ],
        "error_reporting": -1,
        "memory_limit": -1
    },
    "reports": {
        "aggregate_compact": {
            "extends": "aggregate",
            "cols": [ "benchmark", "subject", "mode", "rstdev" ]
        }
    }
}
