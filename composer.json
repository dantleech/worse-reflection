{
    "name": "phpactor/worse-reflection",
    "description": "Lazy AST reflector that is much worse than better",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Leech",
            "email": "daniel@dantleech.com"
        }
    ],
    "require": {
        "microsoft/tolerant-php-parser": "@dev",
        "jetbrains/phpstorm-stubs": "dev-master",
        "psr/log": "^1.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0@stable",
        "symfony/filesystem": "^3.3",
        "phpbench/phpbench": "dev-master",
        "phpactor/class-to-file": "^1.0@dev",
        "phpstan/phpstan": "^0.8.5"
    },
    "autoload": {
        "psr-4": {
            "Phpactor\\WorseReflection\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phpactor\\WorseReflection\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
